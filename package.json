{
  "name": "township-token",
  "version": "2.2.0",
  "description": "create a jwt token from township auth and access data",
  "main": "index.js",
  "scripts": {
    "pub": "np",
    "deps": "ncu",
    "docs:api": "documentation build index.js -f md -o docs/api.md",
    "docs:build": "npm run docs:api",
    "lint": "standard",
    "test:no-lint": "node test.js | tap-spec",
    "test": "npm run lint && npm run test:no-lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/township/township-token.git"
  },
  "keywords": [
    "township",
    "auth",
    "jwt",
    "authentication",
    "authorization",
    "token"
  ],
  "author": "sethvincent",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/township/township-token/issues"
  },
  "homepage": "https://github.com/township/township-token#readme",
  "dependencies": {
    "jsonwebtoken": "^8.4.0",
    "subleveldown": "^3.0.1",
    "through2": "^3.0.0"
  },
  "devDependencies": {
    "documentation": "^9.1.1",
    "level": "^4.0.0",
    "memdb": "^1.3.1",
    "standard": "^12.0.1",
    "tap-spec": "^5.0.0",
    "tape": "^4.9.2",
    "township-access": "^2.0.0",
    "township-auth": "^3.0.0"
  }
